unit UGetMigrations;

interface

uses
  UDefsMigrations, Generics.Collections;

type
  IGetMigrations = interface
    function getMigrations(AStartSeqMigration: Integer): TList<IMigration>;
  end;

type
  TGetMigrations = class
  public
    function getMigrations(AStartSeqMigration: Integer): TList<IMigration>;
  end;

implementation

end.
